PREFIX qb:   <http://purl.org/linked-data/cube#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?gene WHERE {
  [] a qb:ComponentSpecification;
    rdfs:label "Tumor_Sample_Barcode Component";
    qb:measure ?barcode_measure.

  [] a qb:ComponentSpecification;
    rdfs:label "Hugo_Symbol Component";
    qb:measure ?gene_measure.

  ?patient_obs a qb:Observation;
    ?barcode_measure ?barcode;
    ?gene_measure ?gene.
  
  FILTER (regex(?barcode, "TCGA-%{patient}.*"))
}